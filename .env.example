# FastAPI service configuration
FACE_V3_ENV=local
FACE_V3_APP_NAME=Face Search Backend
FACE_V3_API_PREFIX=/api/v1
FACE_V3_API_KEY=
FACE_V3_TRITON_URL=triton:8001
FACE_V3_DETECTION_MODEL_NAME=detection
FACE_V3_EXTRACTION_MODEL_NAME=extraction
FACE_V3_DETECTION_INPUT_WIDTH=640
FACE_V3_DETECTION_INPUT_HEIGHT=640
FACE_V3_MAX_BATCH_SIZE=8
FACE_V3_RERANK_LIBRARY_PATH=/opt/face_backend/libs/librerank_compute.so
FACE_V3_RERANK_THRESHOLD=0.5
FACE_V3_RETURN_ALIGNED=false
FACE_V3_SKIP_EMBEDDING_NORMALIZATION=false
FACE_V3_LOG_LEVEL=INFO

# Uvicorn options
UVICORN_WORKERS=2
